clear; clc; close all;

% lcv,
% (int32)pChan->sv,
% pSV->time,
% pSV->x,
% pSV->y,
% pSV->z,
% pSV->vx,
% pSV->vy,
% pSV->vz);

c = 2.99792458e8;

fname = sprintf('../satellites.tlm');
a = load(fname);
len = length(a)/12;
% len = 268;
a = a(1:12*len,:);

svx = reshape(a(:,4),[12 len]).';
svy = reshape(a(:,5),[12 len]).';
svz = reshape(a(:,6),[12 len]).';

figure(1)
clf(1)
plot(svx)

figure(2)
clf(2)
plot(svy)

figure(3)
clf(3)
plot(svz)
